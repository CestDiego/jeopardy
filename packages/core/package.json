{
  "name": "@rukuma/core",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "typecheck": "tsc -noEmit",
    "db:pull": "sst bind drizzle-kit introspect:pg --introspect-casing=camel",
    "db:push": "sst bind drizzle-kit push:pg",
    "db:branch:get": "sst bind tsx ./src/utils/neon.db get",
    "db:branch:reset": "sst bind tsx ./src/utils/neon.db reset",
    "db:branch:prepare-pr-branch": "sst bind tsx ./src/utils/neon.db prepare-pr-branch",
    "db:branch:delete": "sst bind tsx ./src/utils/neon.db delete",
    "drizzle-studio": "sst bind drizzle-kit studio",
    "drizzle-kit:up": "sst bind drizzle-kit up",
    "migration:create": "drizzle-kit generate:pg",
    "migration:amend": "sst bind drizzle-kit drop && sst bind drizzle-kit generate:pg && sst bind tsx ./src/migrate.ts",
    "migration:check": "sst bind drizzle-kit check:pg",
    "migration:run": "sst bind tsx ./src/migrate.ts"
  },
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "exports": {
    "./": "./src/"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "drizzle-kit": "^0.23.0",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "@aws-sdk/s3-request-presigner": "^3.614.0",
    "@langchain/community": "^0.2.18",
    "@langchain/core": "^0.2.15",
    "drizzle-orm": "^0.32.0",
    "drizzle-valibot": "^0.2.0",
    "pino": "^9.2.0",
    "ts-custom-error": "^3.3.1",
    "ulid": "^2.3.0",
    "valibot": "^0.36.0"
  }
}
